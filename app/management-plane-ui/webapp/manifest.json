{
  "_version": "1.59.0",
  "sap.app": {
    "id": "com.ndbs.managementplaneui",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.14.0",
      "toolsId": "cfca3b93-9ea9-4fab-aef0-2836cd308476"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v2/management-plane/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "resourceRoots": {
      "ui5.antares":"./resources/ui5/antares"
    },
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.125.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {},
        "ui5.antares":{}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.ndbs.managementplaneui.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.f.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "com.ndbs.managementplaneui.view",
        "controlId": "flexibleApp",
        "transition":"slide"
      },
      "routes": [
        {
          "name": "RouteHomepage",
          "pattern": ":?query:",
          "target": [
            "TargetHomepage"
          ]
        },
        {
          "name": "RouteKPIsOverview",
          "pattern": "KPIsOverview/sectionID={sectionID}",
          "target": [
            "TargetKPIsOverview"
          ]
        },
        {
          "name": "RouteKPIs",
          "pattern": "KPIs/sectionID={sectionID}&kpiID={kpiID}",
          "target": [
            "TargetKPIs"
          ]
        },
        {
          "name": "RouteKPIDetails",
          "pattern": "KPIdetails/sectionID={sectionID}&kpiID={kpiID}&subKPI={subKPI}&paragraph={paragraph}&{layout}",
          "layout": "TwoColumnsMidExpanded",
          "target": [
            "TargetKPIDetails",
            "TargetKPIs"
          ]
        },
        {
          "name": "RouteChangeHistory",
          "pattern": "TimeLine/sectionID={sectionID}&kpiID={kpiID}&subKPI={subKPI}&paragraph={paragraph}&{layout}",
          "layout":"ThreeColumnsMidExpanded",
          "target": [
            "TargetChangeHistory",
            "TargetKPIDetails",
            "TargetKPIs"
          ]
        },
        {
          "name": "RouteReportAdministration",
          "pattern": "ReportAdministration",
          "target": [
            "TargetReportAdministration"
          ]
        },
        {
          "name": "RouteUploadReport",
          "pattern": "UploadReport",
          "layout": "TwoColumnsMidExpanded",
          "target": [
            "TargetUploadReport",
            "TargetReportAdministration"
          ]
        },
        {
          "name": "RouteReportDetails",
          "pattern": "ReportDetails/reportID={reportID}&{layout}",
          "layout": "TwoColumnsMidExpanded",
          "target": [
            "TargetReportDetails",
            "TargetReportAdministration"
          ]
        }

      ],
      "targets": {
        "TargetHomepage": {
          "type" :"Component",
          "viewId": "Homepage",
          "viewType": "XML",
          "viewName": "Homepage",
          "transition": "slide",
          "controlAggregation": "beginColumnPages"
        },
        "TargetKPIsOverview": {
          "type" :"Component",
          "viewType": "XML",
          "viewId": "KPIsOverview",
          "viewName": "KPIsOverview",
          "transition": "slide",
          "controlAggregation": "beginColumnPages"
        },
        "TargetKPIs": {
          "type" :"Component",
          "viewId": "KPIs",
          "viewType": "XML",
          "viewName": "KPIs",
          "transition": "slide",
          "controlAggregation": "beginColumnPages",
          "contextPattern": "KPIs/sectionID={sectionID}&kpiID={kpiID}"
        },
        "TargetKPIDetails": {
          "type" :"Component",
          "viewId": "KPIDetails",
          "viewType": "XML",
          "viewName": "KPIDetails",
          "transition": "slide",
          "controlAggregation": "midColumnPages",
          "contextPattern": "KPIdetails/sectionID={sectionID}&kpiID={kpiID}&subKPI={subKPI}&paragraph={paragraph}&{layout}"
        },
        "TargetChangeHistory": {
          "type" :"Component",
          "viewId": "ChangeHistory",
          "viewType": "XML",
          "viewName": "ChangeHistory",
          "transition": "slide",
          "controlAggregation": "endColumnPages",
          "contextPattern": "TimeLine/sectionID={sectionID}&kpiID={kpiID}&subKPI={subKPI}&paragraph={paragraph}&{layout}"
        },
        "TargetReportAdministration": {
          "type" :"Component",
          "viewType": "XML",
          "viewId": "ReportAdministration",
          "viewName": "ReportAdministration",
          "transition": "slide",
          "controlAggregation": "beginColumnPages"
        },
        "TargetUploadReport": {
          "type" :"Component",
          "viewType": "XML",
          "viewId": "Upload",
          "viewName": "Upload",
          "transition": "slide",
          "controlAggregation": "midColumnPages"
        },
        "TargetReportDetails": {
          "type" :"Component",
          "viewType": "XML",
          "viewId": "ReportDetails",
          "viewName": "ReportDetails",
          "transition": "slide",
          "controlAggregation": "midColumnPages",
           "contextPattern": "ReportDetails/reportID{reportID}&{layout}"
        }
      }
    },
    "rootView": {
      "viewName": "com.ndbs.managementplaneui.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
